{
  "name": "TimeTable",
  "version": "1.5.77",
  "description": "Get your TimeTable in your menubar!",
  "license": "MIT",
  "author": "Willyb321",
  "repository": {
    "type": "git",
    "url": "https://github.com/willyb321/electron-menubar-timetable"
  },
  "bugs": {
    "url": "https://github.com/willyb321/electron-menubar-timetable/issues"
  },
  "main": "./main.js",
  "scripts": {
    "lint": "xo",
    "test": "npm run lint",
    "start": "electron .",
    "pack": "build --dir",
  	"release": "build"
  },
  "devDependencies": {
    "electron": "^1.4.6",
    "xo": "^0.17.0",
    "electron-builder": "8.4.1"
  },
  "dependencies": {
    "electron-config": "^0.2.1",
    "menubar": "^5.1.0"
  },
  "xo": {
    "ignores": [
      "test.js"
    ]
  },
"build": {
    "appId": "com.willyb321.electron-menubar-timetable"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "dmg": {
      	"contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
      },
      "linux": {
      "target": [
        "deb"
      ]
    }
  },

      "category": "public.app-category.reference",
      "publish": [
        "github"
      ]
    }
